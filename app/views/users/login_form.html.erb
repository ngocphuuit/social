		
	<!--main-->
	<main class="main" role="main">
		<!--wrap-->
		<div class="wrap clearfix">
			<!--row-->
			<div class="row">
			<!--content-->
				<section class="content center full-width wow fadeInUp">
					<div class="modal container">
						<h3>Login</h3>
						<form action="/users/login" method="POST">
							<div class="f-row">
								<input type="email" name="user[email]" placeholder="Your email" />
							</div>
							<div class="f-row">
								<input type="password" name="user[password]" placeholder="Your password" />
							</div>
							<div class="f-row">
								<input type="checkbox" />
								<label>Remember me next time</label>
							</div>
							<div class="f-row bwrap">
								<input type="submit" value="login" />
							</div>
						</form>
						<p><a href="#">Forgotten password?</a></p>
						<p>Dont have an account yet? <a href="/users/register">Sign up.</a></p>
					</div>
				</section>
				<!--//content-->
			</div>
			<!--//row-->
		</div>
		<!--//wrap-->
	</main>
	<!--//main-->



